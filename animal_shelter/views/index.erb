<h2>Pics of Cute Doggos</h2>
<div id="animal-wrapper">
<% @animals.each do |animal| %>

  <p>Name: <%= animal.name.capitalize() %></p>
  <p>Species: <%= animal.species() %></p>
  <p>Age: <%= animal.age() %></p>
  <p>Adoptable: <%= animal.adoptable() %></p>
  <p>Owner: <%= animal.owner() %></p>
  <hr/>

<% end %>
</div>
